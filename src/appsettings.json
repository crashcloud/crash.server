{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Crash": {
		"SignalR": {
			"CrashHub": {
				"MaximumParallelInvocationsPerClient": "1",
				"MaximumReceiveMessageSize": "1048560",
				"StreamBufferCapacity": "15",
				"KeepAliveInterval": "10",
				"ClientTimeoutInterval" : "10",
				"HandshakeTimeout": "15",
				"EnableDetailedErrors": true
			},
			"Json": {
				"AllowTrailingCommas": true,
				"DefaultIgnoreCondition": "WhenWritingDefault",
				"IgnoreReadOnlyFields": true,
				"IgnoreReadOnlyProperties": true,
				"IncludeFields": false,
				"MaxDepth": "0",
				"PropertyNameCaseInsensitive": true,
				"WriteIndented": false,
				"ReadCommentHandling": "Skip",
				"NumberHandling": "AllowNamedFloatingPointLiterals"
			}
		}
	}
}
	